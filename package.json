{
    "name": "flagwind-web",
    "version": "1.0.0",
    "description": "flagwind web library",
    "author": "jason <jasonsoop@gmail.com>",
    "scripts": {
        "dev": "webpack-dev-server --content-base test/ --open --inline --hot --compress --history-api-fallback --port 8083 --config build/webpack.dev.config.js",
        "dist:debug": "webpack --config build/webpack.dist.debug.config.js",
        "dist:release": "webpack --config build/webpack.dist.release.config.js",
        "dist": "npm run dist:debug && npm run dist:release",
        "unit": "karma start test/unit/karma.conf.js",
        "test": "npm run unit"
    },
    "files": [
        "src",
        "dist"
    ],
    "main": "dist/flagwind-web.js",
    "typings": "dist/typings/flagwind-web.d.ts",
    "keywords": [
        "flagwind",
        "typescript"
    ],
    "license": "MIT",
    "homepage": "http://www.flagwind.org",
    "repository": {
        "type": "git",
        "url": "https://github.com/flagwind/flagwind-web"
    },
    "bugs": {
